<ArrayOfCustomFolderSettings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd ="http://www.w3.org/2001/XMLSchema">
  <!--Example Folder-->
  <CustomFolderSettings FolderId="Uniqe folder identification">
    <FolderEnabled>false</FolderEnabled>
    <FolderDescription>This folder is a test folder</FolderDescription>
    <FolderFilter>*.txt</FolderFilter>
    <FolderPath>C:\Test</FolderPath>
    <FolderIncludeSub>false</FolderIncludeSub>
    <Action>-backup</Action>
    <ActionArguments>-changed archive=true dest=C:\destinationFolder</ActionArguments>
  </CustomFolderSettings>
  <!--Example Folder End-->
  
</ArrayOfCustomFolderSettings>